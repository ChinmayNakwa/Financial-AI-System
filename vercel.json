{
  "version": 2,
  "builds": [
    {
      "src": "backend/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "backend/financial_workflow.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ]
}