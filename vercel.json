{
  "version": 2,
  "builds": [
    {
      "src": "backend/api.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/index.html",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "frontend"
      }
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "backend/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ]
}